<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>logch</servlet-name>
        <servlet-class>logch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logadmin</servlet-name>
        <servlet-class>logadmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_feedback</servlet-name>
        <servlet-class>add_feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_message</servlet-name>
        <servlet-class>add_message</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_movie</servlet-name>
        <servlet-class>add_movie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>movie_change</servlet-name>
        <servlet-class>movie_change</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_adminview</servlet-name>
        <servlet-class>load_adminview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>myservleyt</servlet-name>
        <servlet-class>myservleyt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logch</servlet-name>
        <url-pattern>/logch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logadmin</servlet-name>
        <url-pattern>/logadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_feedback</servlet-name>
        <url-pattern>/add_feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_message</servlet-name>
        <url-pattern>/add_message</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_movie</servlet-name>
        <url-pattern>/add_movie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>movie_change</servlet-name>
        <url-pattern>/movie_change</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_adminview</servlet-name>
        <url-pattern>/load_adminview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>myservleyt</servlet-name>
        <url-pattern>/myservleyt</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
